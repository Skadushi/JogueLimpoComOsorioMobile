{
  "expo": {
    "name": "Jogue Limpo",
    "slug": "JogueLimpo",
    "privacy": "public",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#446D44"
    },
    "updates": {
      "fallbackToCacheTimeout": 30
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "br.gov.rs.osorio.joguelimpo",
      "supportsTablet": true,
      "infoPlist": {
        "NSAppTransportSecurity": {
          "NSAllowsArbitraryLoads": true,
          "NSAllowsArbitraryLoadsForMedia": true,
          "NSAllowsArbitraryLoadsInWebContent": true,
          "NSExceptionAllowsInsecureHTTPLoads": true
        }
      }
    },
    "android": {
      "package": "br.gov.rs.osorio.joguelimpo",
      "permissions": [
        "CAMERA",
        "READ_EXTERNAL_STORAGE", 
        "WRITE_EXTERNAL_STORAGE"
      ]
    },
    "packagerOpts": {
      "sourceExts": [
        "js",
        "json",
        "ts",
        "tsx",
        "jsx"
      ]
    },
    "androidStatusBar": {
      "backgroundColor": "#446D44"
    }
  }
}
